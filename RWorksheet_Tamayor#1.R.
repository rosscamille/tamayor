#Vector

age <- c (34, 28, 22, 36, 27, 18, 52, 39, 42, 29,
          35, 31, 27, 22, 37, 34, 19, 20, 57, 49, 50, 37, 46, 25, 17, 37, 42, 53, 41,
          51, 35, 24, 33, 41)
#1data points
length (age)

#2reciprocal 
reciprocalAge <-1/age
reciprocalAge

#3Assign also new_age <-c (age, 0, age)
NewAge <-c (age, 0, age)
NewAge

#4Sort the value for age 
SortValueAge <- sort(age)
SortValueAge

#5Minimum and Maximum value for age
minimumAge <- min(age)
minimumAge
maximumAge <-max(age)
maximumAge



